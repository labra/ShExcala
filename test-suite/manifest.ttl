@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf:     <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix qt:     <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .
@prefix shext:   <http://www.w3.org/ns/shextest#> .
@prefix ex:     <http://example.org/> .


<>  rdf:type mf:Manifest ;
    rdfs:comment "ShEx tests" ;
    mf:entries
    (
    <#a-string-syntax>
    <#simple-star>
    <#a-string-valid>
#	<#country-observations-valid>
	<#a-string-no-valid>
    ) .

<#a-string-syntax> rdf:type shext:PositiveSyntax ;
   mf:name      "a-string-syntax" ;
   rdfs:comment "Shex Expression with an single arc rule" ;
   shext:status mf:proposed ;
   shext:schema <a-string.shex> ;
   .

<#simple-star> rdf:type shext:PositiveSyntax ;
   mf:name      "simple-star" ;
   rdfs:comment "Shex Expression with an single arc rule and a star" ;
   shext:status mf:proposed ;
   shext:schema <simple-star.shex> ;
   .

<#a-string-valid> rdf:type shext:Valid ;
   mf:name      "a-string-valid" ;
   rdfs:comment "label a with property p and a string" ;
   shext:status mf:proposed ;
   shext:schema   <a-string.shex> ;
   shext:instance <a-string.ttl> ;
   shext:iri      ex:x ;
   shext:shape 	  ex:a 
   .

