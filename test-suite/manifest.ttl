<#a-2string-no-valid> rdf:type shext:NonValid ;
   mf:name      "a-2string-no-valid" ;
   rdfs:comment "a-2string 2 strings for p" ;
   shext:status mf:proposed ;
   shext:schema   <a-string.shex> ;
   shext:instance <a-2string.ttl> ;
   .
   
<#a-string-no-valid> rdf:type shext:NonValid ;
   mf:name      "a-string-no-valid" ;
   rdfs:comment "a-string with no value for p" ;
   shext:status mf:proposed ;
   shext:schema   <a-string.shex> ;
   shext:instance <a-string-no-p.ttl> ;
   .
