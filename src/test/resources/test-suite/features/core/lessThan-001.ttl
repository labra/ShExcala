# baseURI: http://www.w3.org/ns/shacl/test/features/core/lessThan-001

@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<>
	a sh:Graph ;
	sh:shapesGraph <http://www.w3.org/ns/shacl> ;
.


# Shape Definitions -----------------------------------------------------------

ex:MyShape
	a sh:Shape ;
	sh:constraint [
		a sh:LessThanConstraint ;
		sh:predicate1 ex:property1 ;
		sh:predicate2 ex:property2 ;
	] 
.

# Instances -------------------------------------------------------------------

ex:ValidResource1
	sh:nodeShape ex:MyShape ;
	ex:property1 4 ;
	ex:property2 6 ;
.

ex:ValidResource2
	sh:nodeShape ex:MyShape ;
	ex:property1 3.1 ;
	ex:property1 3.2 ;
.

# Invalid: Values cannot be equal
ex:InvalidResource1
	sh:nodeShape ex:MyShape ;
	ex:property1 4 ;
	ex:property2 4 ;
.

# Invalid: One combination does not match
ex:InvalidResource2
	sh:nodeShape ex:MyShape ;
	ex:property1 4 ;
	ex:property1 6;
	ex:property2 5 ;
.

# Invalid: Greater than
ex:InvalidResource3
	sh:nodeShape ex:MyShape ;
	ex:property1 5 ;
	ex:property2 4 ;
.
