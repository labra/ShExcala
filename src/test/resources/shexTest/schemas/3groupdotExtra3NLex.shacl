@prefix xsd:   &lt;http://www.w3.org/2001/XMLSchema#&gt; .

ex:S1  sh:property  [ a             sh:PropertyConstraint ;
                              sh:maxCount   1 ;
                              sh:minCount   1 ;
                              sh:predicate  ex:p3
                            ] ;
        sh:property  [ a             sh:PropertyConstraint ;
                       sh:maxCount   1 ;
                       sh:minCount   1 ;
                       sh:predicate  ex:p2
                     ] ;
        sh:property  [ a             sh:PropertyConstraint ;
                       sh:maxCount   1 ;
                       sh:minCount   1 ;
                       sh:predicate  ex:p1
                     ] ;
        sh:schema    [ a  sh:Schema ] .

